{
  "name": "discord-mod-bot",
  "version": "0.1.5",
  "author": "Death Killer21",
  "description": "Moderation+ - A moderation suite built for discord moderators.",
  "main": "src/index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "deploy:commands": "node src/deploy-commands.js",
    "deploy:commands:global": "node src/deploy-commands-global.js",
    "clear:commands:guild": "node src/clear-commands-guild.js",
    "deploy:bot1:global": "dotenv -e .env -- node src/deploy-commands-global.js",
    "deploy:bot2:global": "dotenv -e .env.bot2 -- node src/deploy-commands-global.js",
    "deploy:all:global": "npm run deploy:bot1:global && npm run deploy:bot2:global"
  },
  "dependencies": {
    "discord.js": "^14.22.1",
    "dotenv": "^16.6.1"
  },
  "devDependencies": {
    "dotenv-cli": "^11.0.0",
    "nodemon": "^3.1.10"
  }
}
